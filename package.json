{
  "name": "laravel-fashion-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npx mix --production --mix-config=webpack.mix.js",
    "dev": "npx mix --mix-config=webpack.mix.js",
    "development": "npx mix --mix-config=webpack.mix.js",
    "watch": "npx mix watch --mix-config=webpack.mix.js",
    "watch-poll": "npx mix watch --mix-config=webpack.mix.js -- --watch-options-poll=1000",
    "hot": "npx mix watch --hot --mix-config=webpack.mix.js",
    "prod": "npm run production",
    "production": "npx mix --production --mix-config=webpack.mix.js"
  },
  "dependencies": {
    "laravel-mix": "^6.0.49",
    "resolve-url-loader": "^5.0.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "postcss": "^8.4.24",
    "axios": "^1.6.4",
    "jquery": "^3.7.1",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5",
    "popper.js": "^1.16.1",
    "@popperjs/core": "^2.11.8"
  },
  "devDependencies": {
    "laravel-vite-plugin": "^1.0"
  }
}
